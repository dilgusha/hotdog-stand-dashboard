POSTGRES_PASSWORD=       # Məs: Sizin Postgres şifrəniz
POSTGRES_DATABASE=       # Məs: postgres

# Full database connection URL
DATABASE_URL=            # Məs: postgresql://USERNAME:PASSWORD@HOST:PORT/DATABASE
